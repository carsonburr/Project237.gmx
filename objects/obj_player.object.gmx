            <string>/// Player Movement

{
    //Moving Left &amp; Right
    if(keyboard_check(vk_right) &amp;&amp; place_free(x+4,y) &amp;&amp; !(keyboard_check(vk_left)))
    {
        if (place_free(x,y+1))
            {
                if(place_free(x+4,y+vspeed))
                {
                    x+=4
                }
            }
        else
        x+=4
    }
    else if (keyboard_check(vk_left) &amp;&amp; place_free(x-4,y) &amp;&amp; !(keyboard_check(vk_right)))
    {
        if (place_free(x,y+1))
            {
                if(place_free(x-4,y+vspeed))
                {
                    x-=4
                }
            }
        else
        x-=4
    }

    //Gravity
    if (place_free(x,y+1))
    {
        gravity = .75
        
        //Max Falling Speed
        if (vspeed &gt;= 10)
        {
            vspeed = 10
        }
    }
    else
    {
        gravity = 0
        
        //Jumping
        if (keyboard_check(vk_up))
        {
            vspeed = -10
        }
    }
}
</string>
